{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "cli": {
    "packageManager": "pnpm",
    "analytics": false
  },
  "newProjectRoot": "projects",
  "projects": {
    "lifecycle-visualizer": {
      "projectType": "library",
      "root": "projects/lifecycle-visualizer",
      "sourceRoot": "projects/lifecycle-visualizer/src",
      "prefix": "lv",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/lifecycle-visualizer/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/lifecycle-visualizer/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/lifecycle-visualizer/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/lifecycle-visualizer/src/test.ts",
            "tsConfig": "projects/lifecycle-visualizer/tsconfig.spec.json",
            "karmaConfig": "projects/lifecycle-visualizer/karma.conf.js"
          }
        }
      }
    }
  }
}
